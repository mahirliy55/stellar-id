{
  "name": "stellar-id",
  "version": "1.3.0",
  "description": "ðŸŒŸ A TypeScript library for generating unique, deterministic star-themed identifiers with real astronomical data integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docs": "typedoc src/index.ts --out docs",
    "clean": "rm -rf dist docs",
    "prepublishOnly": "npm run build",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "keywords": [
    "typescript",
    "id",
    "generator",
    "unique",
    "stars",
    "astronomy",
    "identifier",
    "hash",
    "deterministic",
    "library"
  ],
  "author": "Yusif Jabrayilov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mahirliy55/stellar-id.git"
  },
  "homepage": "https://github.com/mahirliy55/stellar-id#readme",
  "bugs": {
    "url": "https://github.com/mahirliy55/stellar-id/issues"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.10",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  }
}
